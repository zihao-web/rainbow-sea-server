{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/db/(.*)": "<rootDir>/../libs/db/src/$1",
    "@app/db": "<rootDir>/../libs/db/src",
    "@app/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@app/config": "<rootDir>/../libs/config/src",
    "@app/email/(.*)": "<rootDir>/../libs/email/src/$1",
    "@app/email": "<rootDir>/../libs/email/src",
    "@app/tools/(.*)": "<rootDir>/../libs/tools/src/$1",
    "@app/tools": "<rootDir>/../libs/tools/src"
  }
}